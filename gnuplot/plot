#!/usr/bin/gnuplot
set terminal png size 1024,600
set output 'gnuplot/paretoFront.png'
set multiplot
set xlabel "Objective 0"
set grid
set title "Pareto front"
set ylabel "Objective 1"
set size 0.9,0.9
set origin 0.00,0.05
set key center top
plot [0:1][-1:1] '< sort gnuplot/dataPareto.txt' using 1:2 title "Front 0" with lp,\
	"<echo '1.000000 1.000000'" title "Reference point" with points,\
	0 title "Top pareto limit" with lp;
set nomultiplot
reset
